@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>resultPage</title>
    <style>
        .container {
            height: 350px;
            width: 60%;
            margin-top: 80px;
            margin-left: 20%;
            align-items: center;
            border-radius: 10px;
            box-shadow: 0px 0px 10px 0px rgba(0, 0, 0, 0.1);
            padding-top: 25px;
            padding: 40px 25px 25px 25px;
            text-align:center;
           
        }

        .info {
            padding: 20px;
            text-align: center;
        }

        .img img {
            height: 130px;
            width: 130px;
        }

        .button-green {
            background-color: lightgreen;
            padding: 10px 20px;
            border: none;
            border-radius: 5px;
            margin: 5px;
            text-decoration: none;
            color: white;
            font-weight: bold;
            display: block;
            
            align-content:center;
            margin-left: 37%;
            margin-right: 37%;
        }

        
    </style>
</head>
<body>
    @if (!string.IsNullOrEmpty(ViewBag.checkstring) && ViewBag.checkstring == "Ukl")
    {
        <div class="container">
            <div class="img">
            <img src="/css/Images/checklist_10629607.png" />
            </div>
            <div class="info">
                <h3>Voice Liveness Verification Successfull</h3>
            <a class="nav-link button-green" asp-area="" asp-controller="Audio" asp-action="Index">Continue</a><br />
            <a class="nav-link button-green" asp-area="" asp-controller="Audio" asp-action="UploadAudioFile">Retry</a>
            </div>
        </div>
    }
</body>
</html>
